Location_BarC_Function<-function(Data, Title){
ggplot(Data, aes(x=Service, fill=Location)) +
stat_count()+theme_minimal()+
scale_x_continuous(breaks = seq(1,Meal_Day, by = 1))+
ggtitle(Title)+
theme(plot.title = element_text(hjust = 0.5))
}
